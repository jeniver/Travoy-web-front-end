{
  "name": "travoy-web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "npx vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "preview": "npx vite preview",
    "deploy:s3": "npm run build && aws s3 sync dist s3://$S3_BUCKET/ --delete --cache-control 'public, max-age=31536000, immutable' --exclude index.html && aws s3 cp dist/index.html s3://$S3_BUCKET/index.html --cache-control 'no-cache, no-store, must-revalidate' --content-type 'text/html' && aws s3 cp dist/manifest.webmanifest s3://$S3_BUCKET/manifest.webmanifest --cache-control 'no-cache' --content-type 'application/manifest+json' && aws s3 cp dist/sw.js s3://$S3_BUCKET/sw.js --cache-control 'no-cache' --content-type 'application/javascript'",
    "analyze": "npx vite-bundle-analyzer"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "lucide-react": "^0.441.0",
    "three": "^0.170.0",
    "@reduxjs/toolkit": "^2.0.1",
    "react-redux": "^9.0.4",
    "@tanstack/react-query": "^5.17.9",
    "react-intersection-observer": "^9.5.3",
    "react-window": "^1.8.8",
    "react-virtualized-auto-sizer": "^1.0.20",
    "i18next": "^23.7.11",
    "react-i18next": "^13.5.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.18",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.50.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "typescript": "^5.5.4",
    "vite": "^5.2.0",
    "tailwindcss": "3.4.17",
    "autoprefixer": "latest",
    "postcss": "latest",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.17.4"
  }
}